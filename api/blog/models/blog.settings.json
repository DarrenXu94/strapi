{
  "kind": "singleType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "content": {
      "type": "richtext"
    },
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
